

alias do_grep_python='grep -nr -i --include \*.py '
alias do_grep_cpp='grep -nr -i --include \*.h  --include \*.hpp --include \*.cpp --include \*.c'
alias do_grep_sv='grep -nr -i --include \*.sv --include \*.v'
alias do_grep_json='grep -nr -i --include \*.json'

alias do_nice='taskset 0x7 sudo -E nice -n -20 ionice -c1 -n0 sudo -E -u $USER'